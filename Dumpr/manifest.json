{
  "manifest_version": 2,
  "name": "Dumpr",
  "version": "1.0",

  "description": "Dump your modified URL into a new tab!",
  "icons": { "128": "icon128.png" },

  "permissions": [
    "activeTab",
    "storage"
  ],

  "options_page": "options.html",

  "browser_action": {
    "default_title": "Dumpr",
    "default_popup": "popup.html"
  }
}
